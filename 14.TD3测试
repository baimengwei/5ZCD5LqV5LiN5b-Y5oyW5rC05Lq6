pe[0]\n","    try:self.action_size = self.env.action_space.shape[0]\n","    except:self.action_size = 1\n","    \n","    self.memory_width = self.state_size * 2 + self.action_size + 1\n","    self.memory = np.zeros((self.memory_length, self.memory_width))\n","\n","    self.index = 0\n","    self.max_index = 0\n","\n","  def store(self, state, action, reward, next_state):\n","    transacton = np.hstack((state, action, reward, next_state))\n","    self.memory[self.index,:] = transacton\n","\n","    self.index += 1\n","    if self.index % self.memory_length == 0:\n","      self.index = 0\n","    if self.max_index < self.memory_length:\n","      self.max_index += 1\n","  \n","  def sample(self):\n","    choice_random = np.random.choice(self.max_index, self.memory_minibatch)\n","    choice_data = self.memory[choice_random,:]\n","    state = choice_data[:,0:self.state_size]\n","    action = choice_data[:,self.state_size:self.state_size+self.action_size]\n","    reward = choice_data[:,self.state_size+self.action_size:\n","                         self.state_size+self.action_size+1]\n","    next_state = choice_data[:,self.state_size+self.action_size+1:]\n","\n","    return state, action, reward, next_state\n"],"execution_count":63,"outputs":[]},{"cell_type":"markdown","metadata":{"id":"EvaxTXVIcMvG"},"source":["# Critic 网络"]},{"cell_type":"code","metadata":{"id":"IoLSipa5bSo-","executionInfo":{"status":"ok","timestamp":1616310778476,"user_tz":-480,"elapsed":1302,"user":{"displayName":"白梦伟","photoUrl":"","userId":"14318251260292756104"}}},"source":["class critic:\n","  def __init__(self, env, hidden_dimension=100, learning_rate=1e-3, gamma=0.9):\n","    self.env = env\n","    self.hidden_dimension = hidden_dimension\n","    self.learning_rate = learning_rate\n","    self.gamma = gamma\n","\n","    self.state_dimension = self.env.observation_space.shape[0]\n","    try:self.action_dimension = self.env.action_space.shape[0]\n","    except:self.action_dimension = 1\n","\n","    self.model = self.__create_network()\n","    self.optimizer = torch.optim.Adam(self.model.parameters(), lr=self.learning_rate)\n","\n","\n","  def __create_network(self):\n","    class critic_network(torch.nn.Module):\n","      def __init__(self, state_dimension, action_dimension, hidden_dimension):\n","        super().__init__()\n","        self.state_dimension = state_dimension\n","        self.action_dimension = action_dimension\n","        self.hidden_dimension = hidden_dimension\n","\n","        self.layer_state = torch.nn.Linear(self.state_dimension, self.hidden_dimension)\n","        self.layer_action = torch.nn.Linear(self.action_dimension, self.hidden_dimension)\n","        self.layer_output = torch.nn.Linear(self.hidden_dimension, 1)\n","      \n","      def forward(self, state, action):\n","        hidden_state = self.layer_state(state)\n","        hidden_action = self.layer_action(action)\n","        hidden = hidden_state + hidden_action\n","        hidden = torch.nn.functional.relu(hidden)\n","        output = self.layer_output(hidden)\n","        return output\n","    return critic_network(self.state_dimension, self.action_dimension, self.hidden_dimension)\n"],"execution_count":64,"outputs":[]},{"cell_type":"code","metadata":{"id":"3rIuU5iJbSrl","executionInfo":{"status":"ok","timestamp":1616310778476,"user_tz":-480,"elapsed":1298,"user":{"displayName":"白梦伟","photoUrl":"","userId":"14318251260292756104"}}},"source":[""],"execution_count":64,"outputs":[]},{"cell_type":"markdown","metadata":{"id":"yuvtzYdHcQNu"},"source":["# Actor 网络"]},{"cell_type":"code","metadata":{"id":"zJ4Lshp3bSuN","executionInfo":{"status":"ok","timestamp":1616310778476,"user_tz":-480,"elapsed":1295,"user":{"displayName":"白梦伟","photoUrl":"","userId":"14318251260292756104"}}},"source":["class actor:\n","  def __init__(self, env, hidden_dimension=30, learning_rate=1e-3):\n","    self.env = env\n","    self.hidden_dimension = hidden_dimension\n","    self.learning_rate = learning_rate\n","\n","    self.state_dimension = self.env.observation_space.shape[0]\n","    self.action_dimension = self.env.action_space.shape[0]\n","    self.action_bound = [float(self.env.action_space.low), float(self.env.action_space.high)]\n","\n","    self.model = self.__create_network()\n","    self.entropy = torch.nn.CrossEntropyLoss(reduction='none')\n","    self.optimizer = torch.optim.Adam(self.model.parameters(), lr=self.learning_rate)\n","\n","  def __create_network(self):\n","    class actor_network(torch.nn.Module):\n","      def __init__(self, state_dimension, action_dimension, action_bound, hidden_dimension):\n","        super().__init__()\n","        self.state_dimension = state_dimension\n","        self.action_dimension = action_dimension\n","        self.action_bound = action_bound\n","        self.hidden_dimension = hidden_dimension\n","\n","        self.layer_state = torch.nn.Linear(self.state_dimension, self.hidden_dimension)\n","        self.layer_output = torch.nn.Linear(self.hidden_dimension, self.action_dimension)\n","      \n","      def forward(self, state):\n","        hidden = self.layer_state(state)\n","        hidden = torch.nn.functional.relu(hidden)\n","        output = self.layer_output(hidden)\n","        output = torch.tanh(output) * torch.FloatTensor([self.action_bound[1]])\n","        return output\n","    return actor_network(self.state_dimension, self.action_dimension,\n","                   self.action_bound, self.hidden_dimension)"],"execution_count":65,"outputs":[]},{"cell_type":"code","metadata":{"id":"GucIXXl2caiu","executionInfo":{"status":"ok","timestamp":1616310778477,"user_tz":-480,"elapsed":1292,"user":{"displayName":"白梦伟","photoUrl":"","userId":"14318251260292756104"}}},"source":[""],"execution_count":65,"outputs":[]},{"cell_type":"markdown","metadata":{"id":"6UpbccdMchNX"},"source":["# 智能体交互"]},{"cell_type":"code","metadata":{"id":"a_OkaudlcagW","executionInfo":{"status":"ok","timestamp":1616310778822,"user_tz":-480,"elapsed":1634,"user":{"displayName":"白梦伟","photoUrl":"","userId":"14318251260292756104"}}},"source":["class agent_actor_critic:\n","  def __init__(self, env_name=\"Pendulum-v0\", epoch=1000, gamma=0.9, std=3, std_target=3, delay=3):\n","    self.env_name = env_name\n","    self.epoch = epoch\n","    self.gamma = gamma\n","    self.std = std\n","    self.std_target = std_target\n","    self.delay = delay\n","    self.env = gym.make(self.env_name)\n","\n","    self.critic_network1 = critic(self.env)\n","    self.critic_network2 = critic(self.env)\n","    self.actor_network = actor(self.env)\n","    self.critic_network_target1 = critic(self.env)\n","    self.critic_network_target2 = critic(self.env)\n","    self.actor_network_target = actor(self.env)\n","    self.replace_soft(self.critic_network1.model, self.critic_network_target1.model)\n","    self.replace_soft(self.critic_network2.model, self.critic_network_target2.model)\n","    self.replace_soft(self.actor_network.model, self.actor_network_target.model)\n","    self.memory = memory(self.env)\n","\n","  def start_execute(self):\n","    self.statistic_reward = []\n","    self.learn_cnt = 0\n","    for i in range(self.epoch):\n","      state = self.env.reset()\n","      reward_total = 0\n","      while True:\n","        action = self.actor_network.model(torch.from_numpy(state))\n","        action = torch.clip(torch.normal(action, self.std), \n","                             self.actor_network.action_bound[0],\n","                             self.actor_network.action_bound[1])\n","        action = np.array(action.tolist())\n","        next_state, reward, done, info = self.env.step(action)\n","        self.memory.store(state, action, reward, next_state)\n","        reward_total += reward\n","        state = next_state\n","\n","        loss_actor,loss_critic1, loss_critic2 = -100, -100, -100\n","        if self.memory.max_index >= self.memory.memory_length:\n","          # import pdb; pdb.set_trace()\n","          self.std *= 0.9995\n","          self.std_target *= 0.9995\n","          self.learn_cnt += 1\n","          loss_actor, loss_critic1, loss_critic2 = self.learn()\n","        if done:\n","          break\n","      self.statistic_reward.append(reward_total)\n","      print(', epoch %-4d, total_reward %s, loss_actor = %s, loss_critic1 = %s, loss_critic2 = %s, std = %s' \\\n","            %(i, reward_total, loss_actor, loss_critic1, loss_critic2, self.std))\n","\n","  def replace_soft(self, network_from, network_to, tau=0.01):\n","    for target_param, param in zip(network_to.parameters(), network_from.parameters()):\n","      target_param.data.copy_(\n","        target_param.data * (1.0 - tau) + param.data * tau\n","      )\n","\n","  def learn(self):\n","    state ,action ,reward, next_state = self.memory.sample()\n","    state = torch.from_numpy(state)\n","    action = torch.from_numpy(action)\n","    reward = torch.from_numpy(reward)\n","    next_state = torch.from_numpy(next_state)\n","\n","    next_action = self.actor_network_target.model(next_state).detach()\n","    next_action = torch.clip(torch.normal(next_action, self.std_target), \n","                             self.actor_network.action_bound[0],\n","                             self.actor_network.action_bound[1])\n","    \n","    next_value1 = self.critic_network_target1.model(next_state, next_action).detach()\n","    next_value2 = self.critic_network_target2.model(next_state, next_action).detach()\n","\n","    value_target = reward + self.gamma * torch.min(next_value1, next_value2)\n","\n","    value1 = self.critic_network1.model(state, action)\n","    value2 = self.critic_network2.model(state, action)\n","\n","    loss_critic1 = torch.nn.functional.mse_loss(value1, value_target.detach())\n","    loss_critic2 = torch.nn.functional.mse_loss(value2, value_target.detach())\n","    self.critic_network1.optimizer.zero_grad()\n","    self.critic_network2.optimizer.zero_grad()\n","    loss_critic1.backward()\n","    loss_critic2.backward()\n","    self.critic_network1.optimizer.step()\n","    self.critic_network2.optimizer.step()\n","\n","    loss_actor = torch.Tensor([-100])\n","    \n","    if self.learn_cnt % self.delay == 0:\n","      # import pdb; pdb.set_trace()\n","      action = self.actor_network.model(state)\n","      loss_actor = -torch.mean(self.critic_network1.model(state, action))\n","      self.actor_network.optimizer.zero_grad()\n","      loss_actor.backward()\n","      self.actor_network.optimizer.step()\n","\n","      self.replace_soft(self.critic_network1.model, self.critic_network_target1.model)\n","      self.replace_soft(self.critic_network2.model, self.critic_network_target2.model)\n","      self.replace_soft(self.actor_network.model, self.actor_network_target.model)\n","\n","    return loss_actor.tolist(), loss_critic1.tolist(), loss_critic2.tolist()\n"],"execution_count":66,"outputs":[]},{"cell_type":"markdown","metadata":{"id":"vYy03FbtbqnW"},"source":["![image.png](data:image/png;base64,)"]},{"cell_type":"code","metadata":{"id":"kFEHz4fGcadm","executionInfo":{"status":"ok","timestamp":1616310778822,"user_tz":-480,"elapsed":1630,"user":{"displayName":"白梦伟","photoUrl":"","userId":"14318251260292756104"}}},"source":[""],"execution_count":66,"outputs":[]},{"cell_type":"code","metadata":{"id":"dRFlQenVcaKG","colab":{"base_uri":"https://localhost:8080/","height":1000},"executionInfo":{"status":"error","timestamp":1616310906806,"user_tz":-480,"elapsed":129612,"user":{"displayName":"白梦伟","photoUrl":"","userId":"14318251260292756104"}},"outputId":"95c43cf7-40a2-49b9-c863-0368da02b0a9"},"source":["agent_test = agent_actor_critic()\n","agent_test.start_execute()"],"execution_count":67,"outputs":[{"output_type":"stream","text":[", epoch 0   , total_reward -1284.6300985504142, loss_actor = -100, loss_critic1 = -100, loss_critic2 = -100, std = 3\n",", epoch 1   , total_reward -1574.3740713089342, loss_actor = -100, loss_critic1 = -100, loss_critic2 = -100, std = 3\n","